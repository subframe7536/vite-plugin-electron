{
  "name": "vite-plugin-electron",
  "version": "0.29.0",
  "type": "module",
  "description": "Electron üîó Vite",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./plugin": {
      "require": "./dist/plugin.cjs",
      "import": "./dist/plugin.mjs"
    },
    "./simple": {
      "require": "./dist/simple.cjs",
      "import": "./dist/simple.mjs"
    },
    "./package.json": "./package.json",
    "./electron-env": {
      "types": "./electron-env.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-vite/vite-plugin-electron.git"
  },
  "author": "Leo Wang(ËçâÈûãÊ≤°Âè∑) <308487730@qq.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "types": "tsc",
    "test": "vitest run",
    "prepublishOnly": "npm run build && npm run test"
  },
  "peerDependencies": {
    "vite-plugin-electron-renderer": "*"
  },
  "peerDependenciesMeta": {
    "vite-plugin-electron-renderer": {
      "optional": true
    }
  },
  "dependencies": {
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "local-pkg": "^1.1.2",
    "@types/node": "^24.10.4",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.5",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.16"
  },
  "files": [
    "dist",
    "electron-env.d.ts",
    "plugin.d.ts",
    "simple.d.ts"
  ],
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "renderer"
  ],
  "pnpm": {
    "overrides": {
      "vite": "^8.0.0-beta.5"
    }
  }
}
